    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vector-database",
    "vector-search",
    "embeddings",
    "similarity-search",
    "machine-learning",
    "ai",
    "hnsw",
